{
  "name": "bobo-style",
  "type": "module",
  "version": "0.0.0",
  "description": "An old-fashioned UI Component library for Vue3",
  "author": {
    "name": "bernankez"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Bernankez/bobo-style.git"
  },
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    }
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "types": "es/index.d.ts",
  "files": [
    "es",
    "lib",
    "volar.d.ts"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "build:types": "tsx ./scripts/generate-webstorm-types.ts --tsconfig ./tsconfig.esbuild.json && tsx ./scripts/generate-volar-types.ts --tsconfig ./tsconfig.esbuild.json",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.2.6"
  },
  "dependencies": {
    "vue": "^3.2.41"
  },
  "devDependencies": {
    "@bernankez/eslint-config": "^0.1.0",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.0",
    "eslint": "^8.26.0",
    "fs-extra": "^10.1.0",
    "happy-dom": "^7.6.6",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "lodash-es": "^4.17.21",
    "tsx": "^3.11.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.0",
    "vite-plugin-dts": "^1.6.6",
    "vitest": "^0.24.5",
    "vue-tsc": "^1.0.9"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "vite": "^3.0.0"
      }
    }
  },
  "lint-staged": {
    "*": [
      "eslint --fix"
    ]
  }
}
